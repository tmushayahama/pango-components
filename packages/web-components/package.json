{
  "name": "pango-web-components",
  "version": "0.0.1",
  "description": "Web components for pango related",
  "type": "module",
  "author": {
    "name": "Pango"
  },
  "main": "dist/index.cjs.js",
  "types": "dist/types/index.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "collection:main": "dist/collection/index.js",
  "unpkg": "dist/web-components/web-components.esm.js",
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/web-components/web-components.esm.js"
    },
    "./*": {
      "types": "./dist/components/*.d.ts",
      "import": "./dist/components/*.js"
    },
    "./loader": {
      "types": "./loader/index.d.ts",
      "import": "./loader/index.js"
    }
  },
  "files": [
    "dist/",
    "loader/"
  ],
  "scripts": {
    "build": "stencil build",
    "clear": "del-cli .stencil dist loader www",
    "start": "stencil build --dev --watch --serve --no-open",
    "test": "stencil test --spec"
  },
  "dependencies": {
    "clsx": "^2.1.1",
    "colorjs.io": "^0.5.2",
    "immer": "^10.1.1",
    "js-yaml": "^4.1.0",
    "lodash": "^4.17.21",
    "uuid": "^11.0.3"
  },
  "devDependencies": {
    "@stencil/core": "4.33.1",
    "@stencil/sass": "3.0.5",
    "@types/jest": "^29.5.14",
    "@types/js-yaml": "^4.0.9",
    "@types/mdast": "^4.0.4",
    "@types/node": "^22.9.1",
    "jest": "^29.7.0",
    "jest-cli": "^29.7.0",
    "puppeteer": "^23.9.0",
    "remark-frontmatter": "^5.0.0",
    "remark-parse": "^11.0.0",
    "remark-stringify": "^11.0.0",
    "tsx": "^4.19.4",
    "unified": "^11.0.5",
    "unist-util-visit": "^5.0.0"
  },
  "license": "MIT"
}